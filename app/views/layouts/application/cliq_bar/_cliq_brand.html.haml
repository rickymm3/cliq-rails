%button{type:'button', class:'navbar-toggle',data_toggle:'collapse',data_target:'#navbar-collapse-1'}
  %span.sr-only
    Toggle navigation
  %span.icon-bar
  %span.icon-bar
  %span.icon-bar
%ul.nav.navbar-nav
  %li.dropdown
    %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
      %span.cliq-banner Cliq
      %span.caret
    %ul.dropdown-menu{role:'menu'}
      %li=link_to 'Cliq', cliqs_path
      %li.divider
      -cliq_latest = @categories
      -if cliq_latest.count > 0
        -@categories.each do |latest|
          %li
            =link_to latest.name.capitalize, latest
      -else
        %li No cliq info currently