=render "cliqs/cliq_body/search_results", search: @search ||= {}
-unless @search.has_key?('temp')
  =render "layouts/application/cliq_bar/cliq_carousel"
  .history-list
    .container
      %ul.list-inline.align-top.pull-left
        =render "layouts/application/cliq_bar/cliq_history" if @cliq.present?
  .panel.topic-list
    =render "cliqs/cliq_body/topic_list", topics: @topics